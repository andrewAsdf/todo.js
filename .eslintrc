{
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "script"
  },
  "plugins": [
//      "promise"
  ],
  "rules": {
/*
    "promise/always-return": 1,
    "promise/no-return-wrap": 1,
    "promise/param-names": 1,
    "promise/catch-or-return": 1,
    "promise/no-native": 1,
    "promise/no-nesting": 1,
    "promise/no-promise-in-callback": 2,
    "promise/no-callback-in-promise": 2,
    "promise/avoid-new": 0,
*/
    "no-undef": 2,
    "no-unreachable": 2,
    "no-unused-vars": 2,
    "handle-callback-err": 2,
    "callback-return": 2,
    "no-invalid-this": 2,
    "no-extra-semi": 2,
    "semi": [
      2,
      "always"
    ],
    "indent": [
      1,
      "tab",
      {
        "MemberExpression": 0,
        "SwitchCase": 1
      }
    ],

    "no-implicit-coercion": 0,
    "key-spacing": [
      1,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "no-extra-parens": [
      1,
      "all",
      {
        "conditionalAssign": false,
        "nestedBinaryExpressions": false
      }
    ],
    "spaced-comment": [
      0,
      "always",
      {
        "line": {
          "exceptions": [
            "/"
          ]
        }
      }
    ],
    "max-params": [
      1,
      7
    ],


    "comma-dangle": [
      1,
      "never"
    ],
    "no-cond-assign": 1,
    "no-constant-condition": 1,
    "no-control-regex": 1,
    "no-debugger": 1,
    "no-dupe-args": 1,
    "no-dupe-keys": 1,
    "no-duplicate-case": 1,
    "no-empty": 1,
    "no-empty-character-class": 1,
    "no-ex-assign": 1,
    "no-extra-boolean-cast": 1,
    "no-func-assign": 1,
    "no-inner-declarations": 1,
    "no-invalid-regexp": 1,
    "no-irregular-whitespace": 1,
    "no-negated-in-lhs": 1,
    "no-obj-calls": 1,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 1,
    "no-unexpected-multiline": 1,
    "use-isnan": 1,
    "valid-jsdoc": 0,
    "valid-typeof": 1,
    "accessor-pairs": [
      1,
      {
        "getWithoutSet": false
      }
    ],
    "array-callback-return": 1,
    "block-scoped-var": 1,
    "complexity": [
      1,
      10
    ],
    "consistent-return": 1,
    "curly": 1,
    "default-case": 1,
    "dot-location": [
      1,
      "property"
    ],
    "dot-notation": 1,
    "eqeqeq": [
      1,
      "smart"
    ],
    "guard-for-in": 1,
    "no-alert": 1,
    "no-caller": 1,
    "no-case-declarations": 1,
    "no-div-regex": 1,
    "no-else-return": 0,
    "no-empty-function": 1,
    "no-empty-pattern": 1,
    "no-eq-null": 1,
    "no-eval": 1,
    "no-extend-native": 1,
    "no-extra-bind": 1,
    "no-extra-label": 1,
    "no-fallthrough": 1,
    "no-floating-decimal": 1,
    "no-implicit-globals": 2,
    "no-implied-eval": 1,
    "no-iterator": 1,
    "no-labels": [
      1,
      {
        "allowLoop": true,
        "allowSwitch": true
      }
    ],
    "no-lone-blocks": 1,
    "no-loop-func": 0,
    "no-magic-numbers": 0,
    "no-multi-spaces": 0,
    "no-multi-str": 1,
    "no-native-reassign": [
      1,
      {
        "exceptions": [
          "Promise"
        ]
      }
    ],
    "no-new": 1,
    "no-new-wrappers": 1,
    "no-param-reassign": 0,
    "no-proto": 1,
    "no-redeclare": 1,
    "no-return-assign": 1,
    "no-self-assign": 1,
    "no-self-compare": 1,
    "no-sequences": 1,
    "no-throw-literal": 1,
    "no-unmodified-loop-condition": 1,
    "no-unused-expressions": 1,
    "no-unused-labels": 1,
    "no-useless-call": 1,
    "no-useless-concat": 1,
    "no-void": 1,
    "no-warning-comments": [
      0,
      {
        "terms": [
          "todo",
          "fixme",
          "hack"
        ],
        "location": "anywhere"
      }
    ],
    "no-with": 1,
    "radix": 0,
    "vars-on-top": 0,
    "wrap-iife": [
      1,
      "inside"
    ],
    "yoda": 1,
    "global-require": 1,
    "no-mixed-requires": 1,
    "no-new-require": 1,
    "no-path-concat": 1,
    "no-process-exit": 1,
    "array-bracket-spacing": [
      1,
      "never"
    ],
    "block-spacing": [
      1,
      "always"
    ],
    "brace-style": 0,
    "comma-spacing": [
      1,
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": [
      1,
      "last"
    ],
    "camelcase": 1,
    "computed-property-spacing": [
      1,
      "never"
    ],
    "consistent-this": [
      2,
      "self"
    ],
    "func-names": 0,
    "func-style": [
      1,
      "declaration",
      {
        "allowArrowFunctions": true
      }
    ],
    "jsx-quotes": 0,
    "keyword-spacing": 1,
    "linebreak-style": [
      1,
      "unix"
    ],
    "lines-around-comment": 0,
    "max-depth": [
      1,
      3
    ],
    "max-len": [
      0,
      150,
      4
    ],
    "max-nested-callbacks": [
      1,
      4
    ],
    "max-statements": [
      1,
      40,
      {
        "ignoreTopLevelFunctions": true
      }
    ],
    "new-cap": 1,
    "new-parens": 1,
    "newline-after-var": 0,
    "newline-per-chained-call": 0,
    "no-array-constructor": 1,
    "no-inline-comments": 0,
    "no-lonely-if": 1,
    "no-mixed-spaces-and-tabs": [
      1,
      "smart-tabs"
    ],
    "no-multiple-empty-lines": 0,
    "no-negated-condition": 0,
    "no-nested-ternary": 1,
    "no-new-object": 1,
    "no-plusplus": 0,
    "no-spaced-func": 1,
    "no-trailing-spaces": 1,
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 1,
    "no-whitespace-before-property": 1,
    "object-curly-spacing": [
      1,
      "always"
    ],
    "one-var": 0,
    "one-var-declaration-per-line": [
      1,
      "initializations"
    ],
    "operator-assignment": [
      1,
      "always"
    ],
    "operator-linebreak": [
      1,
      "after",
      {
        "overrides": {
          "?": "ignore",
          ":": "ignore"
        }
      }
    ],
    "quote-props": [
      1,
      "consistent"
    ],
    "quotes": 0,
    "strict": [
      1,
      "global"
    ],
    "semi-spacing": [
      1,
      {
        "before": false,
        "after": true
      }
    ],
    "space-before-blocks": 1,
    "space-before-function-paren": [
      1,
      {
        "anonymous": "always",
        "named": "never"
      }
    ],
    "space-in-parens": [
      1,
      "never"
    ],
    "space-infix-ops": 1,
    "space-unary-ops": [
      1,
      {
        "words": true,
        "nonwords": false
      }
    ],
    "unicode-bom": [
      1,
      "never"
    ],
    "arrow-body-style": [
      1,
      "as-needed"
    ],
    "arrow-parens": [
      1,
      "as-needed"
    ],
    "arrow-spacing": 1,
    "constructor-super": 1,
    "generator-star-spacing": [
      1,
      {
        "before": false,
        "after": true
      }
    ],
    "no-class-assign": 1,
    "no-confusing-arrow": 0,
    "no-const-assign": 1,
    "no-dupe-class-members": 1,
    "no-new-symbol": 1,
    "no-this-before-super": 1,
    "no-useless-constructor": 1,
    "prefer-arrow-callback": 0,
    "prefer-const": 0,
    "object-shorthand": 0,
    "prefer-reflect": 0,
    "prefer-rest-params": 0,
    "prefer-spread": 0,
    "prefer-template": 0,
    "require-yield": 1,
    "template-curly-spacing": [
      1,
      "always"
    ],
    "yield-star-spacing": [
      1,
      "after"
    ],
    "no-var": 0
  },
  "env": {
    "mocha": true,
    "node": true,
    "es6": true
  }
}
